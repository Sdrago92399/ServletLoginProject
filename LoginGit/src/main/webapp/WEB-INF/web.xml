<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  
  
  <servlet>
  	<servlet-name>signup</servlet-name>
  	<servlet-class>com.login.Signup</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>signup</servlet-name>
  	<url-pattern>/signup</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  	<servlet-name>login</servlet-name>
  	<servlet-class>com.login.Login</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  	<servlet-name>passReset</servlet-name>
  	<servlet-class>com.login.PassReset</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>passReset</servlet-name>
  	<url-pattern>/passReset</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  	<servlet-name>logout</servlet-name>
  	<servlet-class>com.login.Logout</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>logout</servlet-name>
  	<url-pattern>/logout</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  	<servlet-name>profile</servlet-name>
  	<servlet-class>com.login.Profile</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>profile</servlet-name>
  	<url-pattern>/profile</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  	<servlet-name>google</servlet-name>
  	<servlet-class>com.login.Google</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>google</servlet-name>
  	<url-pattern>/login/google</url-pattern>
  </servlet-mapping>
  
  
    <servlet>
  	<servlet-name>verifyOTP</servlet-name>
  	<servlet-class>com.login.VerifyOTP</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>verifyOTP</servlet-name>
  	<url-pattern>/verifyOTP</url-pattern>
  </servlet-mapping>
  
  
  <display-name>Login</display-name>
  <welcome-file-list>
    <welcome-file>signup.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
</web-app> 